{
    "name": "@docspace/theme",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "vite build"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./v2": "./src/v2/index.ts",
        "./typography": "./src/presets/typography.css.ts"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "main": "dist/index.cjs",
        "module": "dist/index.js",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "require": "./dist/index.cjs"
            },
            "./style.css": "./dist/style.css",
            "./v2": {
                "types": "./dist/v2/index.d.ts",
                "import": "./dist/v2/index.js",
                "require": "./dist/v2/index.cjs"
            },
            "./typography": {
                "types": "./dist/presets/typography.css.d.ts",
                "import": "./dist/presets/typography.js",
                "require": "./dist/presets/typography.cjs"
            }
        },
        "types": "./dist/index.d.ts"
    },
    "devDependencies": {},
    "dependencies": {
        "ajv": "^8.17.1",
        "@vanilla-extract/css": "^1.16.0",
        "@vanilla-extract/vite-plugin": "^4.0.17",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0"
    }
}
